package code {	//@author: Rebecca Vessal	//Instructor: Professor Jefferson	//Date: 29 October 2011	//	//MenuButton.as represents the button for going back to the title screen.  	//	import flash.display.SimpleButton;	import flash.events.MouseEvent;			public class MenuButton extends SimpleButton 	{						public function MenuButton() 		{			cacheAsBitmap = true;			// constructor code			addEventListener(MouseEvent.CLICK, goToTitle);		}				public function goToTitle(e:MouseEvent):void		{			//Remove game elements from the screen.			if((parent as Document).Games.gameLayerArray.length > 0)			{				(parent as Document).Games.resetGame();			}						(parent as Document).gotoAndStop("Title");			removeEventListener(MouseEvent.CLICK, goToTitle);		}							}	}