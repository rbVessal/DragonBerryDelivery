package code{	//@author: Rebecca Vessal	//Instructor: Professor Jefferson	//Date: 15 November 2011	//	//Chicken.as represents a chicken	//	import flash.display.MovieClip;	import sounds.ChickenCall;	import flash.media.SoundChannel;			public class Chicken extends GameElement	{				//Create an instance of chicken call sound		private var chickenSound:ChickenCall;		public var chickenSoundChannel:SoundChannel; 				public function Chicken(xCoordinate:int, yCoordinate:int, widthChicken:int, heightChicken:int) 		{			// constructor code			//Pass along the passed in parameters to game element			super(xCoordinate, yCoordinate, widthChicken, heightChicken);			//Initialize the chicken sound			chickenSound = new ChickenCall();			//Initialize the chicken sound channel and play the chicken sound			chickenSoundChannel = chickenSound.play(0,9999);			cacheAsBitmap = true;			//Play the chicken sound			//chickenSoundChannel.play(0, 9999);		}				//Create an accessor for chickenSound		public function get ChickenSound():ChickenCall		{			return chickenSound;		}		//Create an accessor for chickenSoundChannel		public function get ChickenSoundChannel():SoundChannel		{			return chickenSoundChannel;		}	}	}