package code{	//@author: Rebecca Vessal	//Instructor: Professor Jefferson	//Date: 29 October 2011	//	//EnterButton.as represents the button for going to the Objective screen.  	//	import flash.display.SimpleButton;	import flash.events.MouseEvent;	import flash.display.*;			public class EnterButton extends SimpleButton 	{						public function EnterButton() 		{			cacheAsBitmap = true;			// constructor code			addEventListener(MouseEvent.CLICK, goToObjective);		}				public function goToObjective(e:MouseEvent):void		{			(parent as Document).gotoAndStop("Objective");			removeEventListener(MouseEvent.CLICK, goToObjective);					}			}	}